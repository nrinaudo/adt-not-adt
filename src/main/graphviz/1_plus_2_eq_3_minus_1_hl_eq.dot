digraph {
    one1[label="Num(1)" class="value" style="invis"]
    two[label="Num(2)" class="value" style="invis"]
    add[label="Add" class="operator" style="invis"]
    add -> one1[style="invis"]
    add -> two[style="invis"]

    three[label="Num(3)" class="value" style="invis"]
    one2[label="Num(1)" class="value" style="invis"]
    sub[label="Sub" class="operator" style="invis"]
    sub -> three[style="invis"]
    sub -> one2[style="invis"]

    eq[label="Eq" class="operator"]
    eq -> add[style="invis"]
    eq -> sub[style="invis"]
}
